/* Titulares Paginador */

.pane-vista-titulares {
	position: relative;
}

.pane-vista-titulares .item-list {
	position: absolute;
	top: -10px;
	right: 0px;
}

.pane-vista-titulares .item-list .pager {
	position: relative;
}

.pane-vista-titulares .item-list .pager .pager-previous {
	position: absolute;
	right: 50px;
	width: 17px;
	height: 12px;
}

.pane-vista-titulares .item-list .pager .pager-previous a {
	font-size: 0;
	display: block;
	width: 17px;
	height: 12px;
	background: url("/avnueva/sites/all/themes/avn/images/background-prev.png") no-repeat;
}

.pane-vista-titulares .item-list .pager .pager-current {
	display: none;
}

.pane-vista-titulares .item-list .pager .pager-next {
	position: absolute;
	right: 20px;
	width: 17px;
	height: 12px;
}

.pane-vista-titulares .item-list .pager .pager-next a {
	font-size: 0;
	display: block;
	width: 17px;
	height: 12px;
	background: url("/avnueva/sites/all/themes/avn/images/background-next.png") no-repeat;
}


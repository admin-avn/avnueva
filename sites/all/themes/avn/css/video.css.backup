}

.video2-contenedor .horizontal1 .video-portada .titulo {
	padding: 10px 0 0 50px;
}

.video2-contenedor .horizontal1 .video-portada .titulo a {
	color: white;
	font-size: 12px;
	font-weight: bold;
	text-decoration: none;
}

.video2-contenedor .horizontal1 .video-portada .descripcion {
	color: white;
	font-size: 12px;
	margin: 20px 0 0 50px;
	text-decoration: none;
	width: 200px;
}

.video2-contenedor .horizontal3 .termino {
	width: 110px;
}

.video2-contenedor .horizontal3 .termino,
.video2-contenedor .horizontal3 .imagen {
	float: left;
	margin: 10px 0 0 10px;
}

.video2-contenedor .horizontal3 .imagen {
	margin-right: 10px;
}

.video2-contenedor .horizontal3 .video-portada .titulo {
	float: left;
	padding: 10px 0 0 50px;
}

.video2-contenedor .horizontal3 .video-portada .titulo a {
	color: white;
	font-size: 12px;
	font-weight: bold;
	text-decoration: none;
}

.video2-contenedor .horizontal3 .video-portada .descripcion {
	color: white;
	font-size: 12px;
	float: left;
	margin: 20px 0 0 50px;
	text-decoration: none;
	width: 200px;
}

/* QTVideo2 */

#quicktabs-qtvideo2 {
}

#quicktabs-qtvideo2 ul {
	background: #b60001;
	float: left;
	margin: 0;
	padding: 0;
	width: 200px;
}

#quicktabs-qtvideo2 ul li {
	border-bottom: 1px solid white;
	display: table;
	padding: 0;
	line-height: 60px;
	height: 60px;
	width: 200px;
}

#quicktabs-qtvideo2 ul li a {
	color: #eee;
	font-size: 16px;
	font-weight: bold;
	padding: 5px 10px;
	text-decoration: none;
}

.video-player img.playbutton {
	position: absolute;
	left: 292px;
	top: 222px;
	height: 85px;
	width: 85px;
	-webkit-transition: opacity .5s ease-in-out;
	-moz-transition: opacity .5s ease-in-out;
	-o-transition: opacity .5s ease-in-out;
	transition: opacity .5s ease-in-out;
	opacity: 0.5;
}

.video-player img.playbutton:hover {
	-webkit-transition: opacity .5s ease-in-out;
	-moz-transition: opacity .5s ease-in-out;
	-o-transition: opacity .5s ease-in-out;
	transition: opacity .5s ease-in-out;
	opacity: 1;
}

.video-player img.playbutton2 {
	position: absolute;
	left: 280px;
	top: 120px;
	height: 85px;
	width: 85px;
	-webkit-transition: opacity .5s ease-in-out;
	-moz-transition: opacity .5s ease-in-out;
	-o-transition: opacity .5s ease-in-out;
	transition: opacity .5s ease-in-out;
	opacity: 0.5;
}

.video-player img.playbutton2:hover {
	-webkit-transition: opacity .5s ease-in-out;
	-moz-transition: opacity .5s ease-in-out;
	-o-transition: opacity .5s ease-in-out;
	transition: opacity .5s ease-in-out;
	opacity: 1;
}


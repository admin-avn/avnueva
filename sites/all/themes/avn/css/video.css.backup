@font-face {
        font-family: 'vb';
        src: url("/sites/all/themes/avn/fonts/v.ttf");
}

@font-face {
        font-family: 'vb';
        src: url("/sites/all/themes/avn/fonts/vb.ttf");
}

.video-contenedor {
	padding-top: 10px;
}

.video-contenedor .view-display-id-block_3 {
	background-color: black;
	padding: 10px;
}

.video-contenedor .descripcion-video {
	height: 130px;
	padding: 10px;
}

.descripcion-video .titulo span {
	color: white;
	font-family: verdana, sans-serif;
	font-size: 18px;
}

.descripcion-video .contenido {
	color: white;
	font-family: verdana, sans-serif;	
	font-size: 12px;
}

.video-contenedor .listado-principal-derecho .tiempo {
	color: #1e1e1e;
	font-family: verdana, sans-serif;
	font-size: 11px;
}

.video-contenedor .listado-principal-derecho .contenido {
	color: #1e1e1e;
	font-family: verdana, sans-serif;
	font-size: 11px;
	line-height: 12px;
	width: 163px;
}

.video-contenedor .listado-principal-derecho .contenido p {
	margin: 0;
}

.video-contenedor .views-row-1 .listado-principal-derecho {
	margin-top: 10px;
}

.video-contenedor .views-row-1 .listado-principal-derecho,
.video-contenedor .views-row-2 .listado-principal-derecho,
.video-contenedor .views-row-3 .listado-principal-derecho,
.video-contenedor .views-row-4 .listado-principal-derecho,
.video-contenedor .views-row-5 .listado-principal-derecho {
	border-bottom: 1px solid #1e1e1e;
}

.video-contenedor .listado-principal-derecho {
	padding: 18px 10px;
	background-color: #949292;
	margin: 0 0 0 10px;
}

.video-contenedor .listado-principal-derecho .imagen {
	float: right;
}

.video-contenedor .listado-principal-derecho .imagen a {
	display: block;
	width: 100px;
	height: 80px;
}

.video-contenedor .listado-principal-derecho .titulo {
	line-height: 12px;
	width: 163px;
	float: left;
}

.video-contenedor .listado-principal-derecho .titulo a {
	color: #1e1e1e;
	font-family: verdana, sans-serif;
	font-size: 12px;
	text-decoration: none;
}

.video-contenedor .video-titulo-listado {
	color: white;
	background-color: #bd0800;
	font-family: verdana, sans-serif;
	font-size: 14px;
	margin-left: 10px;
	padding: 5px;
	text-transform: uppercase;
}

.video-contenedor .video-titulo-carrusel {
	color: white;
	background-color: black;
	font-family: verdana, sans-serif;
	font-size: 12px;
	padding: 5px;
	text-transform: uppercase;
}

.jcarousel-skin-video .video-titulo a {
	color: #1e1e1e;
	font-family: verdana, sans-serif;
	font-size: 12px;
	text-decoration: none;
}

/* Paginador */

.listado-derecho-video ul.pager {
	text-align: right;
	background-color: #949292;
	margin: 0 0 0 10px;
	padding: 8px 10px 10px 0;
}

.listado-derecho-video ul.pager li,
.listado-derecho-video ul.pager li a {
        margin: 0;
        padding: 0;
}

.listado-derecho-video ul.pager li a {
	font-family: verdana, sans-serif;
        font-size: 11px;
        text-decoration: none;
}

.listado-derecho-video ul.pager li {
        display: inline-block;
	font-family: verdana, sans-serif;
	font-size: 11px;
}

.listado-derecho-video ul.pager li.pager-current {
        color: white;
	font-family: verdana, sans-serif;
        font-size: 11px;
        width: 16px;
        height: 16px;
}

.listado-derecho-video ul.pager li.pager-item,
.listado-derecho-video ul.pager li.pager-ellipsis {
        color: #75767a;
	font-family: verdana, sans-serif;
        font-size: 11px;
        width: 16px;
        height: 16px;
}

.listado-derecho-video ul.pager li.pager-first,
.listado-derecho-video ul.pager li.pager-last {
	display: none;
}

.listado-derecho-video ul.pager li.pager-previous {
	background: url('/sites/all/themes/avn/images/background-video-listado-prev.png') left center no-repeat;
	width: 7px;
	height: 8px;
}

.listado-derecho-video ul.pager li.pager-next {
	background: url('/sites/all/themes/avn/images/background-video-listado-next.png') left center no-repeat;
	width: 7px;
	height: 8px;
}

.listado-derecho-video ul.pager li.pager-previous,
.listado-derecho-video ul.pager li.pager-next {
	font-size: 0px !important;
}

.listado-derecho-video ul.pager li.pager-previous a,
.listado-derecho-video ul.pager li.pager-next a {
	display: block;
	width: 7px;
	height: 8px;
	font-size: 0px !important;
}

.listado-derecho-video ul.pager li.pager-first a {
        content: "Prueba";
}

#user-login-form .item-list ul li.first {
        list-style: none;
}

#user-login-form .item-list ul li.first a {
        color: white;
        text-decoration: none;
}

/* Video 2 */

.video2-contenedor {
	background: url("/sites/all/themes/avn/images/banner-video.png") no-repeat;
	height: 594px;
}

.video2-contenedor .horizontal1,
